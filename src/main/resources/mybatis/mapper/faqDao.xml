<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.study.springboot.dao.IfaqDao">

<select id="faq" resultType="com.study.springboot.dto.faqDto">
	select * from faq
</select>
<select id="faqpage" resultType="com.study.springboot.dto.faqDto">
		SELECT * FROM
	    (
	        SELECT 
	            ROWNUM AS RNUM, faq.*
	        FROM
	            faq
	        ORDER BY faq_date desc
	    )
		WHERE RNUM BETWEEN #{param1} AND #{param2}
	</select>

</mapper>