<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace = "com.study.springboot.dao.IreplyDao">

<select id="replyView" resultType="com.study.springboot.dto.replyDto">
	select * from reply
</select>

<insert id="replyInsert">
	insert into reply(reply_content,reply_member_id,reply_community_number,reply_number)
	values(#{reply_content},#{reply_member_id},#{reply_community_number},#{reply_number})
</insert>

</mapper>    