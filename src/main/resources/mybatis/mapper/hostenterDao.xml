<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.study.springboot.dao.IhostenterDao">
    	
    	<insert id="insert_hostenter" >
    		insert into hostenter values(hostenter_idx.nextval,#{hostenter_contents_number},#{hostenter_name},#{hostenter_description},
    									#{hostenter_caution},#{hostenter_img},
    									#{hostenter_zipcode}, #{hostenter_location},#{hostenter_loaction_detail}
    									#{hostenter_price},#{hostenter_headcount},#{hostenter_member_id},
    									#{hostenter_bnumber},#{hostenter_onerow})
    	</insert>
    
    
    </mapper>